#!/bin/zsh
# -----------------------------------------------------------------------------
# Constants
# -----------------------------------------------------------------------------
ABSPATH=$(readlink -f $0)
ABSDIR=$(dirname $ABSPATH)
# -----------------------------------------------------------------------------
#
#
#
# -----------------------------------------------------------------------------
# Imports
# -----------------------------------------------------------------------------
source "$ABSDIR/chat-bot-api/config.conf"
source "$CONDA_PATH/etc/profile.d/conda.sh"
# -----------------------------------------------------------------------------
#
#
#
# -----------------------------------------------------------------------------
# Main
# -----------------------------------------------------------------------------
echo "ENV: $CONDA_ENV"
conda activate $CONDA_ENV
jupyter lab
# -----------------------------------------------------------------------------
